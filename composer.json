{
    "name": "digicademy/chf_map",
    "version": "2.0.0",
    "type": "typo3-cms-extension",
    "description": "Geodata and image maps for research data in TYPO3 as part of the CHF",
    "authors": [
        {
            "name": "Jonatan Jalle Steller",
            "email": "jonatan.steller@adwmainz.de",
            "role": "Developer",
            "homepage": "https://www.adwmainz.de"
        },
        {
            "name": "CHF Map contributors"
        }
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://extensions.typo3.org/extension/chf_map",
    "support": {
        "issues": "https://github.com/digicademy-chf/chf_map/issues",
        "source": "https://github.com/digicademy-chf/chf_map",
        "docs": "https://digicademy-chf.github.io/chf_map"
    },
    "keywords": [
        "TYPO3",
        "Map",
        "Geodata",
        "Image map",
        "Location",
        "Leaflet",
        "Data",
        "Research",
        "Academia",
        "Academic"
    ],
    "require": {
        "typo3/cms-core": "^13",
        "typo3/cms-extbase": "^13",
        "typo3/cms-fluid": "^13",
        "typo3/cms-fluid-styled-content": "^13",
        "typo3/cms-rte-ckeditor": "^13",
        "digicademy/chf_base": "^2"
    },
    "suggest": {
        "digicademy/chf_bib": "Adds source relations",
        "digicademy/chf_lex": "Includes distributions",
        "digicademy/chf_pub": "Provides publication relations"
    },
    "autoload": {
        "psr-4": {
            "Digicademy\\CHFMap\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "chf_map"
        }
    }
}